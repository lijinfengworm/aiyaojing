<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\YjAdminUser" table="yj_admin_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userName" type="string" column="user_name" length="255" nullable="true"/>
    <field name="password" type="string" column="password" length="255" nullable="true"/>
    <field name="sex" type="string" column="sex" length="255" nullable="true"/>
    <field name="tel" type="string" column="tel" length="255" nullable="true"/>
    <field name="realName" type="string" column="real_name" length="255" nullable="true"/>
    <field name="auth" type="text" column="auth" length="65535" nullable="true"/>
    <field name="addTime" type="integer" column="add_time" nullable="true"/>
    <field name="lastLoginIp" type="string" column="last_login_ip" length="255" nullable="true"/>
    <field name="lastLoginTime" type="integer" column="last_login_time" nullable="true"/>
    <field name="status" type="boolean" column="status" nullable="true"/>
  </entity>
</doctrine-mapping>
