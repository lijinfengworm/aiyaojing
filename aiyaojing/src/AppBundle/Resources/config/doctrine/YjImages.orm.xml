<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\YjImages" table="yj_images">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cid" type="integer" column="cid" nullable="true"/>
    <field name="uid" type="integer" column="uid" nullable="true"/>
    <field name="originalImage" type="string" column="original_image" length="255" nullable="true"/>
    <field name="mediumImage" type="string" column="medium_image" length="255" nullable="true"/>
    <field name="smallImage" type="string" column="small_image" length="255" nullable="true"/>
    <field name="source" type="boolean" column="source" nullable="true"/>
    <field name="addTime" type="integer" column="add_time" nullable="true"/>
    <field name="isUse" type="boolean" column="is_use" nullable="true"/>
    <field name="label" type="string" column="label" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
