<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\YjUser" table="yj_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userName" type="string" column="user_name" length="255" nullable="true"/>
    <field name="sex" type="boolean" column="sex" nullable="true"/>
    <field name="tel" type="bigint" column="tel" nullable="true"/>
    <field name="realName" type="string" column="real_name" length="255" nullable="true"/>
    <field name="addTime" type="datetime" column="add_time" nullable="true"/>
    <field name="gold" type="integer" column="gold" nullable="true"/>
  </entity>
</doctrine-mapping>
